<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gsbsoft.evenmore.mapper.ApiMapper">
    <select id="getDBHealthy" resultType="int">
        select count(*) from wslt.short_url
    </select>

    <insert id="insertEventPlan" parameterType="Map">
        insert into evenmore_ask(nameInCharge,email,phoneNm,msg)
        VALUES (#{name},#{email},#{phone},#{msg})
    </insert>
</mapper>
